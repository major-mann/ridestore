{
  "name": "frontend-boilerplate-react",
  "version": "0.1.0",
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.1.1",
    "formik": "^2.4.2",
    "primereact": "^9.5.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-scripts": "^5.0.1",
    "recoil": "^0.7.7",
    "web-vitals": "^3.1.0",
    "yup": "^1.2.0"
  },
  "scripts": {
    "build": "react-scripts build",
    "start": "react-scripts start",
    "test": "cypress run",
    "test:jest": "jest"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "react-hooks/exhaustive-deps": "off"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": [
      "./src/setupTests.js"
    ]
  }
}
